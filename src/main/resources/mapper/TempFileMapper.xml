<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.my.wangpan.generator.mapper.TempFileMapper">

    <resultMap id="BaseResultMap" type="com.my.wangpan.generator.domain.TempFile">
            <id property="fileId" column="file_id" jdbcType="INTEGER"/>
            <result property="fileName" column="file_name" jdbcType="VARCHAR"/>
            <result property="size" column="size" jdbcType="VARCHAR"/>
            <result property="uploadTime" column="upload_time" jdbcType="TIMESTAMP"/>
            <result property="filePath" column="file_path" jdbcType="VARCHAR"/>
    </resultMap>

    <sql id="Base_Column_List">
        file_id,file_name,size,
        upload_time,file_path
    </sql>
</mapper>
